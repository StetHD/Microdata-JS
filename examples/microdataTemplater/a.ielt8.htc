<SCRIPT>

if(!this._)this._={};

/*@requared window._ielt8_getAttributes */
//attributes property start
//Save original attributes property if we don't do it
if(!this._.__ielt8_attributes__)this._.__ielt8_attributes__=this.attributes;

getAttributes = window._ielt8_getAttributes;
//attributes property end

/*@requared window._ielt8_Element_proto = {
 *  getclassList : <Function>,
 *  getfirstElementChild : <Function>,
 *  getlastElementChild : <Function>,
 *  getfirstElementChild : <Function>,
 *  getnextElementSibling : <Function>,
 *  getpreviousElementSibling : <Function>,
 *  getchildElementCount : <Function>,
 *  insertAfter : <Function>,
 *  getElementsByClassName : <Function>,
 *  compareDocumentPosition : <Function>
 * }*/
getClassList=window._ielt8_Element_proto.getclassList;
getFirstElementChild=window._ielt8_Element_proto.getfirstElementChild;
getLastElementChild=window._ielt8_Element_proto.getlastElementChild;
getFirstElementChild=window._ielt8_Element_proto.getfirstElementChild;
