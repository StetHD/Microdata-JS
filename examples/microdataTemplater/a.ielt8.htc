if(!this.getElementsByClassName)this.getElementsByClassName=window._ielt8_Element_proto.getElementsByClassName;
if(!this.compareDocumentPosition)this.compareDocumentPosition=window._ielt8_Element_proto.compareDocumentPosition;

/*@requared: window.addEventListener, window.removeEventListener, window.dispatchEvent */
if(!this.addEventListener)this.addEventListener=window.addEventListener;
if(!this.removeEventListener)this.removeEventListener=window.removeEventListener;
if(!this.dispatchEvent)this.dispatchEvent=window.dispatchEvent;

getDPD=function(){return 0x01}
getDPP=function(){return 0x02}
getDPF=function(){return 0x04}
