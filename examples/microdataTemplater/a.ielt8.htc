<PUBLIC:COMPONENT lightWeight="true">
<PUBLIC:PROPERTY NAME="classList" GET="getClassList" />
<PUBLIC:PROPERTY NAME="attributes" GET="getAttributes" />
<PUBLIC:PROPERTY NAME="firstElementChild" GET="getFirstElementChild" />
<PUBLIC:PROPERTY NAME="lastElementChild" GET="getLastElementChild" />
<PUBLIC:PROPERTY NAME="nextElementSibling" GET="getNextElementSibling" />
<PUBLIC:PROPERTY NAME="previousElementSibling" GET="getPreviousElementSibling" />
<PUBLIC:METHOD NAME="insertAfter" />
<SCRIPT>
//@requared window.Utils.Dom.DOMStringCollection

if(!this._)this._={};

//attributes property start
//Save original attributes property if we don't do it
if(!this._.__ielt8_attributes__)this._.__ielt8_attributes__=this.attributes;

function getAttributes(){
var tmp=this._.__ielt8_attributes__,res={length:0};
for(var i=0,l=tmp.length,k=0;i<l;i++)if(tmp[i].specified){
res[k++]=tmp[i];
res[tmp[i].name]=tmp[i];
res.length++;
}
return res;
}
//attributes property end
//attributes property end

function getClassList(){
var thisObj=this;
if(!thisObj._._cachedClassList)thisObj._._cachedClassList=new window.Utils.Dom.DOMStringCollection(thisObj.className,function(){
//thisObj.setAttribute("class", this.value);
thisObj.className=this.value;
})
return thisObj._._cachedClassList;
}

function getFirstElementChild(){
var node = this.firstChild;
while(node&&node.nodeType!=1)node=node.nextSibling;
return node;
}

function getLastElementChild(){
var node = this.lastChild;
while(node&&node.nodeType!=1)node=node.previousSibling;
return node;
}

function getNextElementSibling(){
var node=this;
while(node=node.nextSibling)if(node.nodeType==1)break;
return node;
}

function getPreviousElementSibling(){
var node=this;
while(node=node.previousSibling)if(node.nodeType==1)break;
return node;
}

function insertAfter(a,b){return this.insertBefore(a,b.nextSibling)}
</SCRIPT>
</PUBLIC:COMPONENT>